<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="exportedMarkupFiles" type="DocNet.Console.FileListConfigurationSection, DocNet.Console"/>
  </configSections>
  
  <appSettings>
    <add key="RootDirectoryName" value="output"/>
  </appSettings>

  <exportedMarkupFiles>
    <file name="DocNet.Layout.css"/>
    <file name="DocNet.Theme.css"/>
    <file name="DocNet.Scripts.js"/>
    <file name="highlight.pack.js"/>
    <file name="bootstrap.min.css"/>
    <file name="bootstrap-theme.min.css"/>
    <file name="jquery-1.9.1.min.js"/>
    <file name="bootstrap.js"/>
    <file name="collapse.js"/>
    <file name="glyphicons-halflings-regular.eot"/>
    <file name="glyphicons-halflings-regular.svg"/>
    <file name="glyphicons-halflings-regular.ttf"/>
    <file name="glyphicons-halflings-regular.woff"/>
    <file name="glyphicons-halflings-regular.woff2"/>
    <file name="vs.css"/>
  </exportedMarkupFiles>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>